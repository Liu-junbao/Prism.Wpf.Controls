<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Lb.CustomControls">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Lb.CustomControls;component/Themes/ST.Transitions.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style BasedOn="{StaticResource st.TransitionerBox}" TargetType="local:TransitionerBox"/>
    <Style BasedOn="{StaticResource st.TransitioningContent}" TargetType="local:TransitioningContent" />
    <Style BasedOn="{StaticResource st.TransitionerSlide}" TargetType="local:TransitionerSlide" />
    <Style BasedOn="{StaticResource st.TransitionerSlideBox}" TargetType="local:TransitionerSlideBox" />
</ResourceDictionary>
